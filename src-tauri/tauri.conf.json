{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "trebuchet"
  },
  "tauri": {
    "allowlist": {
      "window": {
        "show": true
      }
    },
    "bundle": {
      "active": false,
      "targets": "all",
      "identifier": "com.gmail.karoyqiu.trebuchet",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "center": true,
        "fileDropEnabled": false,
        "title": "Trebuchet",
        "visible": false,
        "width": 1200,
        "height": 720
      }
    ]
  }
}
